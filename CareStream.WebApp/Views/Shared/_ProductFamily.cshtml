@model List<CareStream.Models.ProductFamilyModel>



<header class="bs-canvas-header p-3 text-white overflow-auto">
    <h4 class="d-inline-block text-dark mb-0 float-left">Add Product Family</h4>
</header>

<div class="bs-canvas-content px-3 py-3">
    <div class="container">
        <div class="row ">
            <table id="OffsetWizDiv" class="table table-striped border" style="width:100%">
                <thead>
                    <tr>
                        <th><input name="select_all" value="1" id="example-select-all" type="checkbox" /></th>
                        <th>Product Family Description</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                <input type="checkbox" name="chkbox_ProductFamily_@item.ProductFamilyId" id="chkbox_ProductFamily_@item.ProductFamilyId" data-memberid="@item.ProductFamilyId" />
                            </td>
                            <td>
                                <label style="text-wrap:normal">@item.ProductFamilyName</label>
                            </td>
                        </tr>
                    }

                </tbody>
            </table>

        </div>
    </div>

</div>


